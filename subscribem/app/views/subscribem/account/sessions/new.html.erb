<h2>Sign In</h2>
<%= form_for @user, url: sessions_url do |f| %>
  <p>
    <%= f.label :email %><br>
    <%= f.email_field :email %>
  </p>
  <p>
    <%= f.label :password %>
    <%= f.password_field :password %>
  </p>
  <p>
    <%= f.submit "Sign In" %>
  </p>
<% end %>
<%= link_to "New User?", sign_up_url %>